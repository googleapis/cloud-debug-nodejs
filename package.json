    "@google/cloud-diagnostics-common": "^0.2.0",
    "acorn": "^2.3.0",
    "coffee-script": "^1.9.3",
    "findit": "^2.0.0",
    "google-auth-library": "^0.9.5",
    "request": "^2.61.0",
    "source-map": "^0.4.4"
  }
}
